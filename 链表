#include<iostream>
using namespace std;
//C风格链表
struct node{
	int data;
	node *next;
}Node;
//构造函数
node *create() {
	node *head, *curr;
	head = new node;
	head->next = NULL;				
	curr = head;
	while (cin >> Node.data) {
		curr->next = new node;
		curr = curr->next;
		curr->data = Node.data;
	}
	curr->next = NULL;
	return head;
}
//操作函数

int main() {
	node *list=create();        
	while (list->next)            
	{
		cout << list->next->data << endl;
		list = list->next;
	}
}

C风格，利用头插/尾插操作建表




